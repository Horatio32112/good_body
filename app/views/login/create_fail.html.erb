<h1>Welcome to Login Page</h1>

<%= form_tag v1_login_path do %>
  <p>
    <%= label_tag :email %><br>
    <%= text_field_tag :email %>
  </p>
 
  <p>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </p>
 
  <%= submit_tag "Login" %>
<% end %>

<% if flash[:error] %>
  <p><%= flash[:error] %></p>
<% end %>
